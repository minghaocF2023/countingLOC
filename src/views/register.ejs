  <body>
    <div class="text-center mt-5">
        <h1 class = "mt-5 mb-5 font-weight-normal">Register</h1>
        <form style="max-width: 400px; margin: auto;">
            <!-- <label for="username">Username:</label><br> -->
            <input type="text" id="username" name="username" placeholder="Enter username" class="form-control"><br>
            <!-- <label for="password">Password:</label><br> -->
            <input type="password" id="password" name="pwd" placeholder="Enter password" class="form-control"><br>
            <div class = "d-grid gap-2">
                <button type="button" id="register_button" class="button-styled btn btn-lg btn-primary mt-5">Register</button>
                <button type="button" id="register_button" class="button-styled btn btn-lg btn-primary mt-5">Back to Home</button>
            </div>
            
        </form>
